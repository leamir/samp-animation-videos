{
  "name": "test-anim-recording",
  "version": "1.0.0",
  "description": "",
  "author": "Leamir",
  "license": "MIT",
  "dependencies": {
    "pawn-lang/openmp-stdlib": "^0.3.7"
  },
  "devDependencies": {},
  "entry": "gamemodes/test-anim-recording.pwn",
  "output": "gamemodes/test-anim-recording.amx",
  "scripts": {
    "build": "pawnctl build",
    "start": "pawnctl start",
    "build:start": "pawnctl build && pawnctl start",
    "install": "pawnctl install",
    "uninstall": "pawnctl uninstall"
  },
  "runtime": "openmp",
  "legacy": false,
  "compiler": {
    "input": "gamemodes/test-anim-recording.pwn",
    "output": "gamemodes/test-anim-recording.amx",
    "includes": [
      "includes",
      "gamemodes"
    ],
    "constants": {
      "MAX_PLAYERS": 50,
      "DEBUG": 1
    },
    "options": [
      "-d3",
      "-;+",
      "-(+",
      "-\\+",
      "-Z+"
    ]
  }
}